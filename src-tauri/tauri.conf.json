{
  "$schema": "../../node_modules/@tauri-apps/cli/schema.json",
  "identifier": "com.tradingapp.dev",
  "build": {
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "bundle": {
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png"
    ]
    
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Trading App",
        "width": 1200,
        "height": 920,
        "visible": true,
        "resizable": true
      }
    ]
  }
}